<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ida:Tenant" value="use your tenant login id" />
    <add key="ida:ClientId" value="put the ClientID" />
    <add key="ida:RedirectUri" value="put the redirect URI used for the authentication" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/{0}" />
    <add key="todo:TodoListBaseAddress" value="https://localhost:44321/" />
